{
	"manifest_version": 3,
	"name": "My Extension",
	"version": "1.0",
	"description": "A basic Chrome extension using Manifest V3",
	"permissions": [
		"activeTab",
		"scripting"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
{
	"manifest_version": 3,
	"name": "n0code Tweaks",
	"version": "1.0",
	"description": "A simple extension to make the n0code courses site a little more interesting.",
	"permissions": [
		"activeTab",
		"scripting"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		
	},
	"content_scripts": [
		{
			"matches": ["https://n0code.net/work/teaching/courses/index.html"],
			"js": ["content.js"]
		}
	]
}